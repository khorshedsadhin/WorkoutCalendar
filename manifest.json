{
  "manifest_version": 3,
  "name": "Workout Calendar",
  "version": "1.0",
  "description": "A personal workout calendar to track your routines.",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_title": "Open Workout Calendar"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
